# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

# Node
/node_modules
/API/node_modules
npm-debug.log
yarn-error.log

[*.ts]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
